<script>
  $(function() {
    $('#accessibilityTools .showMenu').on('click', function() {
      var $menu = $('#accessibilityTools .menu'),
          h = $menu.height();
      if ($menu.hasClass('stateClosed')) {
        // メニューを開く処理
        $menu.height(0).removeClass('stateClosed');
        setTimeout(function() {
          $menu.addClass('stateOpen').height(h);
        }, 10);
        setTimeout(function() {
          $menu.height('');
        }, 300);
      } else {
        // メニューを閉じる処理
        $menu.height(h);
        setTimeout(function() {
          $menu.removeClass('stateOpen').height(0);
        }, 10);
        setTimeout(function() {
          $menu.addClass('stateClosed').height('');
        }, 300);
      }
    });
  });
</script>
