<script>
  $(function() {

    // キーボード操作でカテゴリー名がフォーカスを受け取ったら、アクティブクラスを付与して
    // サブカテゴリーを展開する。
    $('#globalNav .category h2 a').on('focus', function() {
      $('#globalNav .stateActive').removeClass('stateActive');
      $(this).parents('.category').addClass('stateActive');
    });

    // 最後のカテゴリーのサブカテゴリーからフォーカスが外れたら、サブカテゴリーを閉じる
    $('#globalNav .category.bosai a').last().on('blur', function() {
      $('#globalNav .stateActive').removeClass('stateActive');
    });

    // マウス操作の場合は付与されているアクティブクラスを除去
    $('#globalNav .category').on('mouseover', function() {
      $('#globalNav .stateActive').removeClass('stateActive');
    });

  });
</script>           <!-- グローバルナビ -->
